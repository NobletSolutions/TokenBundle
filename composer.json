{
  "name": "ns/token-bundle",
  "type": "symfony-bundle",
  "description": "This bundle provides a JWT token integration with symfony",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Nathanael David Noblet",
      "email": "nathanael@noblet.ca"
    }
  ],
  "require": {
    "php": ">=8.3",
    "ext-json": "*",
    "symfony/framework-bundle": "^6.0|^7.0",
    "symfony/http-client": "^6.0|^7.0",
    "twig/twig": "^2.0|^3.0",
    "lcobucci/jwt": "^4.0|^5.0",
    "lcobucci/clock": "^3.0",
    "codercat/jwk-to-pem": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.0",
    "liip/rmt": "^1.7",
    "rector/rector": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "NS\\TokenBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "NS\\TokenBundle\\Tests\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "5.0.x-dev"
    }
  }
}
