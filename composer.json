{
    "name": "ns/token-bundle",
    "type": "symfony-bundle",
    "description": "This bundle provides a JWT token integration with symfony",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Nathanael David Noblet",
            "email": "nathanael@noblet.ca"
        }
    ],
    "require": {
        "php":                      ">=5.5.9",
        "symfony/framework-bundle": "^2.8|^3.0|^4.0|^5.0",
        "twig/twig":                "^2.0|^3.0",
        "lcobucci/jwt":             "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0|^7.0"
    },
    "autoload": {
        "psr-4": { "NS\\TokenBundle\\": "src" }
    },
    "autoload-dev": {
	"psr-4": { "NS\\TokenBundle\\Tests\\": "tests" }
    }
}

